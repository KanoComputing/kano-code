{
  "description": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/KanoComputing/kano-code.git"
  },
  "name": "@kano/code",
  "version": "3.0.0-rc.5",
  "main": "index.js",
  "scripts": {
    "typedoc": "typedoc",
    "tsc": "tsc",
    "rollup": "rollup",
    "build:def": "tsc --allowJs false --declaration --emitDeclarationOnly || exit 0",
    "build:src": "tsc",
    "build": "yarn clean && yarn vendor && yarn build:src && yarn build:def",
    "clean": "rimraf ./dist",
    "serve": "es6-server",
    "docs": "yarn typedoc --options tdconfig.json",
    "serve-test": "web-tester serve dist/**/*.test.js",
    "test": "web-tester run dist/**/*.test.js",
    "ci:test": "yarn run --silent test --reporter xunit > test-results.xml",
    "coverage": "web-tester cover dist/**/*.test.js --exclude ./dist/test/**/*",
    "ci:coverage": "yarn run --silent coverage -r cobertura -r markdown",
    "prepublishOnly": "yarn build",
    "vendor": "yarn vendor:monaco",
    "vendor:monaco": "yarn transform-monaco-css && yarn bundle-monaco-workers && yarn copy-monaco-sources",
    "copy-monaco-sources": "node ./scripts/copy-monaco.js",
    "transform-monaco-css": "node ./scripts/transform-monaco.js",
    "bundle-monaco-worker:editor": "rollup node_modules/monaco-editor/esm/vs/editor/editor.worker.js -f iife --name editorWorker -o ./dist/vendor/monaco-workers/editor.worker.js",
    "bundle-monaco-worker:ts": "rollup node_modules/monaco-editor/esm/vs/language/typescript/ts.worker.js -f iife --name tsWorker -o ./dist/vendor/monaco-workers/ts.worker.js",
    "bundle-monaco-workers": "yarn bundle-monaco-worker:editor && yarn bundle-monaco-worker:ts"
  },
  "publishConfig": {
    "access": "restricted"
  },
  "author": "Paul Varache <perso@paulvarache.ninja>",
  "license": "ISC",
  "devDependencies": {
    "@kano/es6-server": "^1.0.3",
    "@kano/web-tester": "^1.0.0-alpha.14",
    "@polymer/iron-test-helpers": "^3.0.1",
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.5",
    "@types/sinon": "^7.0.6",
    "@webcomponents/shadycss": "^1.9.1",
    "@webcomponents/webcomponentsjs": "^2.2.8",
    "fast-glob": "^2.2.6",
    "mkdirp": "^0.5.1",
    "rimraf": "^2.6.3",
    "rollup": "^1.10.1",
    "sinon": "^7.2.4",
    "typedoc": "^0.14.2",
    "typescript": "^3.2.4"
  },
  "dependencies": {
    "@kano/common": "^1.0.2",
    "@kano/icons": "^0.0.1-alpha.3",
    "@kano/icons-rendering": "^0.0.1-alpha.1",
    "@kano/kwc-blockly": "^3.1.1",
    "@kano/kwc-color-picker": "^3.0.0-beta.6",
    "@kano/kwc-icons": "^3.0.0-beta.3",
    "@kano/styles": "^3.0.6",
    "@kano/telemetry": "^1.0.0",
    "@polymer/decorators": "^3.0.0",
    "@polymer/iron-a11y-keys": "^3.0.0-pre.18",
    "@polymer/iron-icon": "^3.0.0-pre.18",
    "@polymer/iron-icons": "^3.0.0-pre.19",
    "@polymer/iron-iconset-svg": "^3.0.0-pre.18",
    "@polymer/iron-image": "^3.0.0-pre.18",
    "@polymer/iron-pages": "^3.0.0-pre.18",
    "@polymer/marked-element": "^3.0.0-pre.18",
    "@polymer/paper-dialog": "^3.0.0-pre.18",
    "@polymer/paper-dialog-behavior": "^3.0.1",
    "@polymer/polymer": "^3.0.0",
    "challenge-engine": "^1.0.1",
    "gif.js": "git+ssh://git@github.com:jnordberg/gif.js.git",
    "js-beautify": "^1.7.5",
    "lit-element": "^2.0.1",
    "lit-html": "^1.0.0",
    "marked": "^0.6.1",
    "monaco-editor": "^0.16.2",
    "omggif": "^1.0.9",
    "prismjs": "^1.15.0",
    "tone": "^0.12.80",
    "twemoji-min": "git+ssh://git@github.com:KanoComputing/twemoji-min.git"
  }
}
