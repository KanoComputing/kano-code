{
	"steps": [
		{
			"tooltips": [
				{
					"location": "add-part-button",
					"position": "top",
					"text": "We'll start by adding parts to our app.<br>Click here to see all the parts."
				}
			],
			"validation": {
				"open-parts": true
			}
		},
		{
			"tooltips": [
				{
					"location": "sidebar.parts.part-button",
					"position": "right",
					"text": "Drag the Button to your app"
				}
			],
			"highlight": "sidebar.parts.part-button",
			"arrow": {
				"source": "sidebar.parts.part-button",
				"target": "left-panel",
				"size": 120
			},
			"validation": {
				"add-part": {
					"type": "button",
					"id": "part_0"
				}
			}
		},
		{
			"tooltips": [
				{
					"location": "add-part-button",
					"position": "top",
					"text": "All the parts are here, let's code!<br>Hit that button"
				}
			],
			"validation": {
				"close-parts": true
			}
		},
		{
			"tooltips": [
				{
					"location": {
						"block": {
							"rawId": "default_part_event_id"
						}
					},
					"position": "top",
					"text": "Change to <kano-value-preview>button.clicked</kano-value-preview>"
				}
			],
			"validation": {
				"blockly": {
					"value": {
						"target": {
							"rawId": "default_part_event_id"
						},
						"value": "button.clicked"
					}
				}
			}
		},
		{
			"tooltips": [
				{
					"location": "left-panel",
					"position": "right",
					"text": "You can now test the app",
					"next_button": true
				}
			]
		}
	],
	"parts": [
		"button"
	],
	"modules": [],
	"variables": [],
	"mode": "normal"
}
